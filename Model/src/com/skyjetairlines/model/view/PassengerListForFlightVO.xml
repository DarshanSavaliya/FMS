<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PassengerListForFlightVO"
  Version="12.2.1.22.48"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="com.skyjetairlines.model.view.PassengerListForFlightVORowImpl"
  ComponentClass="com.skyjetairlines.model.view.PassengerListForFlightVOImpl"
  AccessMode="DEFAULT"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.skyjetairlines.model.view.PassengerListForFlightVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="b_flightno"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT
    PASSENGERS.PASSENEGER_NAME Passenger_name,PASSENGERS.PHONE Phone_number
FROM
    PASSENGERS
INNER JOIN
    TICKETS ON PASSENGERS.PASSENGER_ID = TICKETS.PASSENGER_ID
INNER JOIN
    TICKET_FLIGHTS ON TICKETS.TICKET_ID = TICKET_FLIGHTS.TICKET_ID
INNER JOIN
    FLIGHTS ON TICKET_FLIGHTS.FLIGHT_ID = FLIGHTS.FLIGHT_ID
WHERE FLIGHTS.FLIGHTNO = :b_flightno AND  
TO_CHAR(cast(SYSDATE as date), 'YYYY-MM-DD') = TO_CHAR(cast(FLIGHTS.ACTUAL_DEPARTURE as date), 'YYYY-MM-DD')]]></SQLQuery>
  <ViewAttribute
    Name="PassengerName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PASSENGER_NAME"
    Expression="PASSENGER_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="PhoneNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PHONE_NUMBER"
    Expression="PHONE_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.skyjetairlines.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
