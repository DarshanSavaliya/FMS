<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TotalRevenueFlightsVO"
  Version="12.2.1.22.48"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="com.skyjetairlines.model.view.TotalRevenueFlightsVORowImpl"
  ComponentClass="com.skyjetairlines.model.view.TotalRevenueFlightsVOImpl"
  AccessMode="DEFAULT"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.skyjetairlines.model.view.TotalRevenueFlightsVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="b_departureDate"
    Kind="where"
    Type="java.sql.Timestamp"/>
  <SQLQuery><![CDATA[SELECT 
    FLIGHTS.FLIGHTNO, TO_CHAR(cast(FLIGHTS.ACTUAL_DEPARTURE as date),'YYYY-MM-DD') Departure_Date, 
    SUM(TICKET_FLIGHTS.AMOUNT) Revenue
FROM 
    FLIGHTS,TICKET_FLIGHTS
WHERE FLIGHTS.FLIGHT_ID = TICKET_FLIGHTS.FLIGHT_ID
GROUP BY TO_CHAR(cast(FLIGHTS.ACTUAL_DEPARTURE as date), 'YYYY-MM-DD'),FLIGHTS.FLIGHTNO
HAVING TO_CHAR(cast(FLIGHTS.ACTUAL_DEPARTURE as date), 'YYYY-MM-DD') = TO_CHAR(cast(:b_departureDate as date), 'YYYY-MM-DD')]]></SQLQuery>
  <ViewAttribute
    Name="Flightno"
