  <SQLQuery><![CDATA[SELECT 
    FLIGHTS.DEPARTURE_AIRPORT DEPARTURE_AIRPORT,
    FLIGHTS.ARRIVAL_AIRPORT ARRIVAL_AIRPORT,
    (SUM(FLIGHTS.TOTAL_SEATS - FLIGHTS.AVAILABLE_SEATS)/SUM(FLIGHTS.TOTAL_SEATS))*100 Capacity_utilization
FROM 
    FLIGHTS
WHERE FLIGHTS.DEPARTURE_AIRPORT = :b_departureAirport AND FLIGHTS.ARRIVAL_AIRPORT = :b_arrivalAirport 
GROUP BY FLIGHTS.DEPARTURE_AIRPORT,FLIGHTS.ARRIVAL_AIRPORT]]></SQLQuery>
