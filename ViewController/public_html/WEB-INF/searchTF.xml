<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="searchTF">
    <default-activity>searchFlightsViewCriteria</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__4">
      <name>numberOfSeats</name>
      <value>#{pageFlowScope.flightSearchQueryBean.numberOfSeats}</value>
      <class>java.lang.Integer</class>
      <required/>
    </input-parameter-definition>
    <input-parameter-definition id="__5">
      <name>bookingDate</name>
      <value>#{pageFlowScope.flightSearchQueryBean.bookingDate}</value>
      <class>java.util.Date</class>
      <required/>
    </input-parameter-definition>
    <managed-bean id="__17">
      <managed-bean-name>flightSearchQueryBean</managed-bean-name>
      <managed-bean-class>com.skyjetairlines.view.bean.FlightSearchQueryBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="searchResult">
      <page>/searchResult.jsf</page>
    </view>
    <view id="flightDetails">
      <page>/flightDetails.jsf</page>
    </view>
    <method-call id="searchFlightsViewCriteria">
      <method>#{bindings.searchFlightsWithParams.execute}</method>
      <outcome>
        <fixed-outcome>viewResult</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-return id="home">
      <outcome>
        <name>home</name>
      </outcome>
    </task-flow-return>
    <task-flow-call id="bookingTF">
      <task-flow-reference>
        <document>/WEB-INF/bookingTF.xml</document>
        <id>bookingTF</id>
      </task-flow-reference>
      <input-parameter id="__9">
        <name>numberOfSeats</name>
        <value>#{pageFlowScope.flightSearchQueryBean.numberOfSeats}</value>
        <pass-by-value/>
      </input-parameter>
    </task-flow-call>
    <control-flow-rule id="__8">
      <from-activity-id>searchResult</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>showDetails</from-outcome>
        <to-activity-id>flightDetails</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__1">
        <from-outcome>search</from-outcome>
        <to-activity-id>searchFlightsViewCriteria</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__11">
      <from-activity-id>searchFlightsViewCriteria</from-activity-id>
      <control-flow-case id="__12">
        <from-outcome>viewResult</from-outcome>
        <to-activity-id>searchResult</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__2">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>home</from-outcome>
        <to-activity-id>home</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>flightDetails</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>bookTickets</from-outcome>
        <to-activity-id>bookingTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__13">
        <from-outcome>backToResults</from-outcome>
        <to-activity-id>searchResult</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
