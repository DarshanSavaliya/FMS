<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="createPassengerTF">
    <default-activity>CreateInsert</default-activity>
    <transaction>
      <new-transaction/>
    </transaction>
    <data-control-scope>
      <isolated/>
    </data-control-scope>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <view id="createPassenger">
      <page>/createPassenger.jsf</page>
    </view>
    <task-flow-return id="cancel">
      <outcome>
        <name>cancel</name>
        <rollback/>
      </outcome>
    </task-flow-return>
    <task-flow-return id="save">
      <outcome>
        <name>save</name>
        <commit/>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>createPassenger</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>createPassenger</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>cancel</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>save</from-outcome>
        <to-activity-id>save</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
