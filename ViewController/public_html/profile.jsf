<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html>
<f:view xmlns:f="http://java.sun.com/jsf/core" xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
    <af:document title="Skyjet Airlines || Profile" id="d1">
        <af:messages id="m1"/>
        <af:form id="f1">
            <af:pageTemplate viewId="/mainTemplateDef.jsf" id="pt1">
                <f:facet name="body">
                    <af:outputText value="Manage your Profile" id="ot1"
                                   inlineStyle="font-size:large; font-weight:bold;"/>
                    <af:button text="Notification" id="b15" inlineStyle="margin-left: 1250px;">
                        <af:showPopupBehavior popupId="notification"/>
                    </af:button>
                    <af:popup childCreation="deferred" autoCancel="enabled" id="notification">
                        <af:dialog id="d4" type="none">
                            <f:facet name="buttonBar">
                                <af:button actionListener="#{bindings.Commit.execute}" text="Save"
                                           disabled="false" id="b16"/>
                            </f:facet>
                            <af:table value="#{bindings.NotificationsInstanceForCurrentCustomer.collectionModel}"
                                      var="row" rows="#{bindings.NotificationsInstanceForCurrentCustomer.rangeSize}"
                                      emptyText="#{bindings.NotificationsInstanceForCurrentCustomer.viewable ? 'No data to display.' : 'Access Denied.'}"
                                      rowBandingInterval="0"
                                      fetchSize="#{bindings.NotificationsInstanceForCurrentCustomer.rangeSize}" id="t4"
                                      partialTriggers="b17">
                                <af:column headerText="#{bindings.NotificationsInstanceForCurrentCustomer.hints.Message.label}"
                                           id="c12" width="500">
                                    <af:inputText value="#{row.bindings.Message.inputValue}"
                                                  label="#{bindings.NotificationsInstanceForCurrentCustomer.hints.Message.label}"
                                                  required="#{bindings.NotificationsInstanceForCurrentCustomer.hints.Message.mandatory}"
                                                  columns="#{bindings.NotificationsInstanceForCurrentCustomer.hints.Message.displayWidth}"
                                                  maximumLength="#{bindings.NotificationsInstanceForCurrentCustomer.hints.Message.precision}"
                                                  shortDesc="#{bindings.NotificationsInstanceForCurrentCustomer.hints.Message.tooltip}"
                                                  id="it12">
                                        <f:validator binding="#{row.bindings.Message.validator}"/>
                                    </af:inputText>
                                </af:column>
                                <af:column id="c13">
                                    <af:button actionListener="#{bindings.Delete1.execute}" text="Mark as read"
                                               disabled="#{!bindings.Delete1.enabled}" id="b17"/>
                                </af:column>
                            </af:table>
                        </af:dialog>
                    </af:popup>
                    <af:panelTabbed position="left" id="pt2" verticalTabMode="iconAndText"
                                    minTabSize="200px">
                        <af:showDetailItem id="profile" text="Profile" disclosed="true" immediate="true">
                            <af:panelFormLayout id="pfl1">
                                <af:inputText value="#{bindings.FirstName.inputValue}"
                                              label="#{bindings.FirstName.hints.label}"
                                              required="#{bindings.FirstName.hints.mandatory}"
                                              columns="#{bindings.FirstName.hints.displayWidth}"
                                              maximumLength="#{bindings.FirstName.hints.precision}"
                                              shortDesc="#{bindings.FirstName.hints.tooltip}" id="it1"
                                              autoSubmit="true">
                                    <f:validator binding="#{bindings.FirstName.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.LastName.inputValue}"
                                              label="#{bindings.LastName.hints.label}"
                                              required="#{bindings.LastName.hints.mandatory}"
                                              columns="#{bindings.LastName.hints.displayWidth}"
                                              maximumLength="#{bindings.LastName.hints.precision}"
                                              shortDesc="#{bindings.LastName.hints.tooltip}" id="it2" autoSubmit="true">
                                    <f:validator binding="#{bindings.LastName.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Email.inputValue}" label="#{bindings.Email.hints.label}"
                                              required="#{bindings.Email.hints.mandatory}"
                                              columns="#{bindings.Email.hints.displayWidth}"
                                              maximumLength="#{bindings.Email.hints.precision}"
                                              shortDesc="#{bindings.Email.hints.tooltip}" id="it3" autoSubmit="true">
                                    <f:validator binding="#{bindings.Email.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Phone.inputValue}" label="#{bindings.Phone.hints.label}"
                                              required="#{bindings.Phone.hints.mandatory}"
                                              columns="#{bindings.Phone.hints.displayWidth}"
                                              maximumLength="#{bindings.Phone.hints.precision}"
                                              shortDesc="#{bindings.Phone.hints.tooltip}" id="it4" autoSubmit="true">
                                    <f:validator binding="#{bindings.Phone.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Address.inputValue}"
                                              label="#{bindings.Address.hints.label}"
                                              required="#{bindings.Address.hints.mandatory}"
                                              columns="#{bindings.Address.hints.displayWidth}"
                                              maximumLength="#{bindings.Address.hints.precision}"
                                              shortDesc="#{bindings.Address.hints.tooltip}" id="it5" autoSubmit="true">
                                    <f:validator binding="#{bindings.Address.validator}"/>
                                </af:inputText>
                                <af:inputDate value="#{bindings.DateOfBirth.inputValue}"
                                              label="#{bindings.DateOfBirth.hints.label}"
                                              required="#{bindings.DateOfBirth.hints.mandatory}"
                                              columns="#{bindings.DateOfBirth.hints.displayWidth}"
                                              shortDesc="#{bindings.DateOfBirth.hints.tooltip}" id="id1"
                                              autoSubmit="true">
                                    <f:validator binding="#{bindings.DateOfBirth.validator}"/>
                                    <af:convertDateTime pattern="#{bindings.DateOfBirth.format}"/>
                                </af:inputDate>
                                <af:button actionListener="#{bindings.Commit.execute}" text="Save"
                                           disabled="#{!bindings.Commit.enabled}" id="b1"
                                           partialTriggers="it1 it2 it3 it4 it5 id1"/>
                            </af:panelFormLayout>
                        </af:showDetailItem>
                        <af:showDetailItem text="Bookings" id="sdi1" immediate="true">
                            <af:panelCollection id="pc3">
                                <f:facet name="menus"/>
                                <f:facet name="toolbar"/>
                                <f:facet name="statusbar"/>
                                <af:table value="#{bindings.BookingsInstanceForCurrentCustomer.collectionModel}"
                                          var="row" rows="#{bindings.BookingsInstanceForCurrentCustomer.rangeSize}"
                                          emptyText="#{bindings.BookingsInstanceForCurrentCustomer.viewable ? 'No data to display.' : 'Access Denied.'}"
                                          rowBandingInterval="0"
                                          selectedRowKeys="#{bindings.BookingsInstanceForCurrentCustomer.collectionModel.selectedRow}"
                                          selectionListener="#{bindings.BookingsInstanceForCurrentCustomer.collectionModel.makeCurrent}"
                                          rowSelection="single"
                                          fetchSize="#{bindings.BookingsInstanceForCurrentCustomer.rangeSize}" id="t1"
                                          maxClientRows="10" autoHeightRows="10" scrollPolicy="page"
                                          varStatus="rowStat" columnStretching="column:c4" styleClass="AFStretchWidth">
                                    <af:column id="c16" width="75" align="center">
                                        <af:outputText value="#{rowStat.index+1}"
                                                       shortDesc="#{bindings.BookingsInstanceForCurrentCustomer.hints.BookingId.tooltip}"
                                                       id="ot5"/>
                                    </af:column>
                                    <af:column headerText="#{bindings.BookingsInstanceForCurrentCustomer.hints.BookingDate.label}"
                                               id="c2" width="150" align="center">
                                        <af:outputText value="#{row.BookingDate}"
                                                       shortDesc="#{bindings.BookingsInstanceForCurrentCustomer.hints.BookingDate.tooltip}"
                                                       id="ot3">
                                            <af:convertDateTime pattern="#{bindings.BookingsInstanceForCurrentCustomer.hints.BookingDate.format}"/>
                                        </af:outputText>
                                    </af:column>
                                    <af:column headerText="#{bindings.BookingsInstanceForCurrentCustomer.hints.TotalAmount.label}"
                                               id="c3" width="150" align="left">
                                        <af:outputText value="#{row.TotalAmount}"
                                                       shortDesc="#{bindings.BookingsInstanceForCurrentCustomer.hints.TotalAmount.tooltip}"
                                                       id="ot4">
                                            <af:convertNumber groupingUsed="false"
                                                              pattern="#{bindings.BookingsInstanceForCurrentCustomer.hints.TotalAmount.format}"/>
                                        </af:outputText>
                                    </af:column>
                                    <af:column id="c4" headerText="Show booking" align="center">
                                        <af:button text="Details" id="b14" action="showBooking"/>
                                    </af:column>
                                </af:table>
                            </af:panelCollection>
                        </af:showDetailItem>
                        <af:showDetailItem text="Passengers" id="sdi2" immediate="true">
                            <af:panelCollection id="pc1">
                                <f:facet name="menus"/>
                                <f:facet name="toolbar">
                                    <af:toolbar id="t5">
                                        <af:button actionListener="#{bindings.CreateInsert.execute}" text="Add"
                                                   disabled="false" id="b6"/>
                                        <af:button actionListener="#{bindings.Delete.execute}" text="Delete"
                                                   disabled="#{!bindings.Delete.enabled}" id="b5"/>
                                        <af:button actionListener="#{bindings.Commit.execute}" text="Save"
                                                   disabled="false" id="b7"/>
                                    </af:toolbar>
                                </f:facet>
                                <f:facet name="statusbar"/>
