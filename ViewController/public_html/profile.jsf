                                                    <af:button actionListener="#{bindings.Rollback.execute}"
                                                               text="Cancel" disabled="false" immediate="true" id="b10">
                                                        <af:resetActionListener/>
                                                    </af:button>
                                                </f:facet>
                                            </af:dialog>
                                        </af:popup>
                                    </af:column>
                                </af:table>
                            </af:panelCollection>
                        </af:showDetailItem>
                    </af:panelTabbed>
                </f:facet>
            </af:pageTemplate>
        </af:form>
    </af:document>
</f:view>