<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.2.1.22.48" id="bookingsManagementPageDef"
                Package="com.skyjetairlines.view.pageDefs">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="BookingsInstance" RangeSize="25" DataControl="FlightManagementSystemAMDataControl"
              id="BookingsInstanceIterator"/>
    <searchRegion Criteria="BookingsVOCriteria" Customizer="oracle.jbo.uicli.binding.JUSearchBindingCustomizer"
                  Binds="BookingsInstanceIterator" id="BookingsVOCriteriaQuery"/>
    <iterator Binds="TicketsInstanceForBookings" RangeSize="25" DataControl="FlightManagementSystemAMDataControl"
              id="TicketsInstanceForBookingsIterator"/>
  </executables>
  <bindings>
    <tree IterBinding="BookingsInstanceIterator" id="BookingsInstance">
      <nodeDefinition DefName="com.skyjetairlines.model.view.BookingsVO" Name="BookingsInstance0">
        <AttrNames>
          <Item Value="BookingId"/>
          <Item Value="CustomerId"/>
          <Item Value="BookingDate"/>
          <Item Value="TotalAmount"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <tree IterBinding="TicketsInstanceForBookingsIterator" id="TicketsInstanceForBookings">
      <nodeDefinition DefName="com.skyjetairlines.model.view.TicketsVO" Name="TicketsInstanceForBookings0">
        <AttrNames>
          <Item Value="TicketId"/>
          <Item Value="BookingId"/>
          <Item Value="PassengerId"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
  </bindings>
</pageDefinition>
