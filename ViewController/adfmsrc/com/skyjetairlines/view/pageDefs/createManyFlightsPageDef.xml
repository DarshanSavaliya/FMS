<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.2.1.22.48" id="createManyFlightsPageDef"
                Package="com.skyjetairlines.view.pageDefs">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="DepartureAirportsLOVInstance" RangeSize="25" DataControl="FlightManagementSystemAMDataControl"
              id="DepartureAirportsLOVInstanceIterator"/>
    <iterator Binds="ArrivalAirportsLOVInstance" RangeSize="25" DataControl="FlightManagementSystemAMDataControl"
              id="ArrivalAirportsLOVInstanceIterator"/>
    <iterator Binds="FlightsInstance" RangeSize="10" DataControl="FlightManagementSystemAMDataControl"
              id="FlightsInstanceIterator"/>
  </executables>
  <bindings>
    <list IterBinding="DepartureAirportsLOVInstanceIterator" ListOperMode="navigation"
          ListIter="DepartureAirportsLOVInstanceIterator" id="DepartureAirportsLOVInstance" DTSupportsMRU="true"
          SelectItemValueMode="ListObject">
      <AttrNames>
        <Item Value="AirportCode"/>
        <Item Value="AirportName"/>
        <Item Value="City"/>
      </AttrNames>
    </list>
    <list IterBinding="ArrivalAirportsLOVInstanceIterator" ListOperMode="navigation"
          ListIter="ArrivalAirportsLOVInstanceIterator" id="ArrivalAirportsLOVInstance" DTSupportsMRU="true"
          SelectItemValueMode="ListObject">
      <AttrNames>
        <Item Value="AirportCode"/>
        <Item Value="AirportName"/>
        <Item Value="City"/>
      </AttrNames>
    </list>
    <action IterBinding="FlightsInstanceIterator" id="CreateFlight"
            InstanceName="FlightManagementSystemAMDataControl.FlightsInstance"
            DataControl="FlightManagementSystemAMDataControl" RequiresUpdateModel="true" Action="createInsertRow"/>
    <action id="Commit" DataControl="FlightManagementSystemAMDataControl" RequiresUpdateModel="true"
            Action="commitTransaction"/>
  </bindings>
</pageDefinition>
