<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.2.1.22.48" id="flightDetailsPageDef"
                Package="com.skyjetairlines.view.pageDefs">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="FlightsInstance" RangeSize="25" DataControl="FlightManagementSystemAMDataControl"
              id="FlightsInstanceIterator"/>
  </executables>
  <bindings>
    <attributeValues IterBinding="FlightsInstanceIterator" id="Flightno">
      <AttrNames>
        <Item Value="Flightno"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="FlightsInstanceIterator" id="ScheduledDeparture">
      <AttrNames>
        <Item Value="ScheduledDeparture"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="FlightsInstanceIterator" id="ScheduledArrival">
      <AttrNames>
        <Item Value="ScheduledArrival"/>
      </AttrNames>
    </attributeValues>
    <list IterBinding="FlightsInstanceIterator" StaticList="false" Uses="LOV_DepartureAirport" id="DepartureAirport"
          DTSupportsMRU="false" SelectItemValueMode="ListObject"/>
    <list IterBinding="FlightsInstanceIterator" StaticList="false" Uses="LOV_ArrivalAirport" id="ArrivalAirport"
          DTSupportsMRU="false" SelectItemValueMode="ListObject"/>
    <attributeValues IterBinding="FlightsInstanceIterator" id="AvailableSeats">
      <AttrNames>
        <Item Value="AvailableSeats"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="FlightsInstanceIterator" id="Duration">
      <AttrNames>
        <Item Value="Duration"/>
      </AttrNames>
    </attributeValues>
  </bindings>
</pageDefinition>
